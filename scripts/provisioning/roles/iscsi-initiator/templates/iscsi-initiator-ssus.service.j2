[Unit]
Description=iSCSI initiator for SSUs target
Requires=iscsi-initiator-ssu1.service iscsi-initiator-ssu2.service
After=iscsi-initiator-ssu1.service iscsi-initiator-ssu2.service

[Service]
Type=oneshot
ExecStart=/bin/true
SyslogIdentifier=iscsi-initiator-ssus
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
